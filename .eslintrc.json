{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "no-console": "off",
    "camelcase": "off",
    "max-len": ["off", { "ignoreComments": true }],
    "no-unused-vars": 0,
    "no-restricted-syntax": 0
  }
}
